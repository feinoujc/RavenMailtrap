<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <appSettings>
    <add key="ServerBehavior" value="RavenMailtrap.Service.ForwardingBehavior, RavenMailtrap.Service" />
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network">
        <network host="forward_to_this_smtp_server" />
      </smtp>
    </mailSettings>
  </system.net>
  <startup>
    <supportedRuntime version="v2.0.50727" />
  </startup>
</configuration>
